#!/usr/bin/env ruby
require 'thor'

require File.expand_path('../generators/active_merchant_generator', __dir__)

Dir[File.expand_path('..', __dir__) + '/generators/*/*.rb'].each do |generator|
  require generator
end

class Generate < Thor
  register(GatewayGenerator, 'gateway', 'gateway NAME', 'Generates a new gateway.')
end

Generate.start
